<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<!-- camelCase snake_case의 자동 변환 설정 -->
  <settings>
    <setting name="mapUnderscoreToCamelCase" value="true"/>
  </settings>
  <typeAliases>
<!--    <typeAlias type="com.hoho.springstudy.database.chap01.Person" alias="person"/>-->
    <typeAlias type="org.nmfw.foodietree.domain.customer.entity.Customer" alias="customer"/>
    <typeAlias type="org.nmfw.foodietree.domain.customer.dto.resp.CustomerMyPageDto" alias="CustomerMyPageDto"/>
    <typeAlias type="org.nmfw.foodietree.domain.customer.dto.resp.CustomerPreferenceAreaDto" alias="CustomerPreferenceAreaDto"/>
    <typeAlias type="org.nmfw.foodietree.domain.customer.dto.resp.CustomerPreferenceFoodDto" alias="CustomerPreferenceFoodDto"/>
  </typeAliases>
  <mappers>
    <mapper resource="mappers/StoreMapper.xml" />
      <mapper resource="mappers/LoginIdCheckMapper.xml"/>
      <mapper resource="mappers/CustomerMyPageMapper.xml"/>
  </mappers>

</configuration>